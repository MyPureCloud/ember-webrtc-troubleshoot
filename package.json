{
  "name": "ember-webrtc-troubleshoot",
  "version": "3.0.1",
  "description": "An ember addon for adding webrtc-troubleshooting to your application",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build",
    "start": "ember server",
    "test": "node_modules/.bin/semistandard blueprints/**/*.js tests/**/*.js addon/**/*.js app/**/*.js && ember test",
    "gh-pages": "ember build --environment=production --output-path=docs"
  },
  "repository": "https://github.com/MyPureCloud/ember-webrtc-troubleshoot",
  "engines": {
    "node": ">= 4.4.0"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "broccoli-json-module": "0.0.2",
    "broccoli-merge-trees": "^1.1.1",
    "broccoli-source": "^1.1.0",
    "ember-cli-babel": "^5.1.5",
    "ember-cli-htmlbars": "^1.0.3"
  },
  "devDependencies": {
    "broccoli-asset-rev": "^2.2.0",
    "ember-ajax": "2.5.3",
    "ember-cli": "^2.9.0",
    "ember-cli-app-version": "^2.0.1",
    "ember-cli-dependency-checker": "^1.2.0",
    "ember-cli-htmlbars-inline-precompile": "^0.3.1",
    "ember-cli-inject-live-reload": "^1.3.1",
    "ember-cli-qunit": "^3.0.2",
    "ember-cli-release": "0.2.9",
    "ember-cli-sri": "^2.0.0",
    "ember-cli-uglify": "^1.2.0",
    "ember-data": "^2.3.0",
    "ember-disable-prototype-extensions": "^1.1.0",
    "ember-disable-proxy-controllers": "^1.0.1",
    "ember-export-application-global": "^1.0.4",
    "ember-intl": "^2.4.1",
    "ember-load-initializers": "^0.6.0",
    "ember-resolver": "^2.0.3",
    "ember-try": "^0.2.8",
    "fs-promise": "^1.0.0",
    "loader.js": "^4.0.0",
    "pre-push": "^0.1.1",
    "semistandard": "^9.1.0"
  },
  "keywords": [
    "ember-addon"
  ],
  "pre-push": [
    "test"
  ],
  "ember-addon": {
    "configPath": "tests/dummy/config"
  },
  "semistandard": {
    "ignore": [
      "/app/translations/webrtc-troubleshoot"
    ]
  }
}
