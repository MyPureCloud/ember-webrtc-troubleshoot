{
  "name": "ember-webrtc-troubleshoot",
  "version": "8.0.2",
  "description": "An ember addon for adding webrtc-troubleshooting to your application",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build",
    "start": "ember server",
    "coverage": "COVERAGE=true ember test",
    "test": "semistandard blueprints/**/*.js tests/**/*.js addon/**/*.js app/**/*.js && npm run coverage",
    "test:only": "ember test",
    "posttest": "cat ./coverage/lcov.info | coveralls",
    "install-chromium": "node node_modules/ember-chromium/download-chrome.js"
  },
  "repository": "https://github.com/MyPureCloud/ember-webrtc-troubleshoot",
  "engines": {
    "node": ">= 4.4.0"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "broccoli-json-module": "1.0.0",
    "broccoli-merge-trees": "^3.0.0",
    "broccoli-source": "^1.1.0",
    "ember-cli-babel": "^6.14.1",
    "ember-cli-htmlbars": "^1.0.3",
    "webrtc-troubleshoot": "^9.0.0"
  },
  "devDependencies": {
    "broccoli-asset-rev": "^2.2.0",
    "broccoli-funnel": "^1.1.0",
    "coveralls": "^3.0.0",
    "ember-chromium": "^3.0.1",
    "ember-cli": "^3.2.0",
    "ember-cli-app-version": "^3.1.3",
    "ember-cli-code-coverage": "0.4.2",
    "ember-cli-dependency-checker": "^3.0.0",
    "ember-cli-htmlbars-inline-precompile": "^1.0.3",
    "ember-cli-inject-live-reload": "^1.3.1",
    "ember-cli-qunit": "^4.3.0",
    "ember-cli-release": "0.2.9",
    "ember-cli-sri": "^2.0.0",
    "ember-cli-test-loader": "^2.2.0",
    "ember-cli-uglify": "^2.0.0",
    "ember-disable-prototype-extensions": "^1.1.0",
    "ember-intl": "^2.26.2",
    "ember-load-initializers": "^1.1.0",
    "ember-resolver": "^4.5.5",
    "ember-try": "^0.2.8",
    "fs-promise": "^2.0.3",
    "loader.js": "~4.6.0",
    "pre-push": "^0.1.1",
    "semistandard": "^12.0.0",
    "webrtc-adapter": "^6.0.0"
  },
  "keywords": [
    "ember-addon"
  ],
  "pre-push": [
    "test"
  ],
  "ember-addon": {
    "configPath": "tests/dummy/config"
  },
  "semistandard": {
    "ignore": [
      "**/dist",
      "**/tmp",
      "**/coverage"
    ]
  }
}
