{
  "name": "ember-webrtc-troubleshoot",
  "version": "6.4.0",
  "description": "An ember addon for adding webrtc-troubleshooting to your application",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build",
    "start": "ember server",
    "coverage": "COVERAGE=true ember test",
    "test": "semistandard blueprints/**/*.js tests/**/*.js addon/**/*.js app/**/*.js && npm run coverage",
    "test:only": "ember test",
    "posttest": "cat ./coverage/lcov.info | coveralls",
    "deploy:gh-pages": "ember github-pages:commit --message \"Deploy gh-pages from commit $(git rev-parse HEAD)\"; git push; git checkout -"
  },
  "repository": "https://github.com/MyPureCloud/ember-webrtc-troubleshoot",
  "engines": {
    "node": ">= 4.4.0"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "broccoli-json-module": "1.0.0",
    "broccoli-merge-trees": "^1.1.1",
    "broccoli-source": "^1.1.0",
    "ember-cli-babel": "^5.1.5",
    "ember-cli-htmlbars": "^1.0.3",
    "webrtc-troubleshoot": "^6.1.1"
  },
  "devDependencies": {
    "broccoli-asset-rev": "^2.2.0",
    "broccoli-funnel": "^1.1.0",
    "coveralls": "^3.0.0",
    "ember-cli": "^2.9.0",
    "ember-cli-app-version": "^2.0.1",
    "ember-cli-code-coverage": "0.4.2",
    "ember-cli-dependency-checker": "^2.0.0",
    "ember-cli-github-pages": "^0.1.2",
    "ember-cli-htmlbars-inline-precompile": "^0.3.1",
    "ember-cli-inject-live-reload": "^1.3.1",
    "ember-cli-qunit": "^3.0.2",
    "ember-cli-release": "0.2.9",
    "ember-cli-sri": "^2.0.0",
    "ember-cli-uglify": "^2.0.0",
    "ember-disable-prototype-extensions": "^1.1.0",
    "ember-disable-proxy-controllers": "^1.0.1",
    "ember-export-application-global": "^1.0.4",
    "ember-intl": "^2.26.2",
    "ember-load-initializers": "^0.6.0",
    "ember-resolver": "^2.0.3",
    "ember-try": "^0.2.8",
    "fs-promise": "^2.0.3",
    "loader.js": "^4.5.1",
    "pre-push": "^0.1.1",
    "semistandard": "^11.0.0",
    "webrtc-adapter": "^6.0.3"
  },
  "keywords": [
    "ember-addon"
  ],
  "pre-push": [
    "test"
  ],
  "ember-addon": {
    "configPath": "tests/dummy/config"
  },
  "semistandard": {
    "ignore": [
      "**/dist",
      "**/tmp",
      "**/coverage"
    ]
  }
}
